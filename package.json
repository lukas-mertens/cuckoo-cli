{
  "name": "cuckoo-cli",
  "description": "An unofficial cli for cuckoo.team",
  "version": "0.0.0",
  "author": "Lukas Mertens",
  "bin": {
    "cuckoo-cli": "./bin/run"
  },
  "bugs": "https://github.com/lukas-mertens/cuckoo-cli/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-autocomplete": "^1.3.10",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.12",
    "chalk": "^4.1.0",
    "node-notifier": "^10.0.0",
    "socket.io-client": "^2.5.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.11",
    "@types/node-notifier": "^8.0.2",
    "@types/socket.io-client": "^3.0.0",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2.7.0",
    "prettier-eslint": "^13.0.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/lukas-mertens/cuckoo-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "cuckoo-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "lukas-mertens/cuckoo-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "package": "npx oclif pack:tarballs",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
